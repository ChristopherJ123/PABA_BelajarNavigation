<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_utama"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/detailProfileFragment"
        android:name="uts.c14230260.paba_mynavigation.DetailProfileFragment"
        android:label="Detail Profile"
        tools:layout="@layout/fragment_detail_profile" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="uts.c14230260.paba_mynavigation.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="uts.c14230260.paba_mynavigation.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_detailProfileFragment"
            app:destination="@id/detailProfileFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="uts.c14230260.paba_mynavigation.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" />
</navigation>